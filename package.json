{
  "name": "tana-cli",
  "version": "0.1.7",
  "module": "main.ts",
  "type": "module",
  "workspaces": [
    "services/ledger",
    "services/identity",
    "services/queue",
    "services/notifications",
    "services/mesh",
    "services/t4"
  ],
  "scripts": {
    "dev": "bun run ./main.ts",
    "make": "bun build ./main.ts --compile --outfile dist/tana && cd services/mesh && bun run build && cd ../t4 && bun run build && cd ../..",
    "make:cli": "bun build ./main.ts --compile --outfile dist/tana",
    "make:mesh": "cd services/mesh && bun run build",
    "make:t4": "cd services/t4 && bun run build",
    "db:up": "docker-compose up -d",
    "db:down": "docker-compose down",
    "db:restart": "docker-compose restart",
    "db:logs": "docker-compose logs -f"
  },
  "devDependencies": {
    "@types/bun": "^1.1.9",
    "@types/jsonwebtoken": "^9.0.7"
  },
  "peerDependencies": {
    "typescript": "^5.6.2"
  },
  "dependencies": {
    "@helia/json": "^5.0.1",
    "@hono/zod-validator": "^0.2.0",
    "@ipld/dag-cbor": "^9.2.5",
    "@noble/ed25519": "^3.0.0",
    "@tananetwork/crypto": "^0.1.0",
    "ansi-colors": "^4.1.3",
    "base64url": "^3.0.1",
    "chalk": "^5.3.0",
    "commander": "^14.0.2",
    "drizzle-orm": "^0.30.0",
    "helia": "^6.0.6",
    "hono": "^4.0.0",
    "inquirer": "^12.10.0",
    "ioredis": "^5.8.2",
    "js-toml": "^1.0.0",
    "jsonwebtoken": "^9.0.2",
    "multiformats": "^13.4.1",
    "nanoid": "^5.0.7",
    "nanospinner": "^1.1.0",
    "pg": "^8.16.3",
    "postgres": "^3.4.0",
    "zod": "^3.22.0"
  },
  "trustedDependencies": [
    "core-js-pure",
    "node-datachannel"
  ]
}
